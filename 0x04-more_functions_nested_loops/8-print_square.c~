#include "main.h"

/**
 * print_square - function draws a square
 * @size: the size of the square
 * Return: void
 */

void print_square(int size)
{
	int s;
	int sl;
	for (s = size; s > 0; s--)
	{
		for (sl = size; s > 0; sl--)
			_putchar('#');
		_putchar('\n');
	}
	if (size <= 0)
		_putchar('\n');
}
