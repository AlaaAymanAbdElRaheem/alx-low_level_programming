#include "lists.h"

/**
 * print_listint - function that prints all the elements of a list_t list.
 * @h: a singly linked list
 * Return: the number of nodes
 */

size_t print_listint(const list_t *h)
{
	size_t count = 0;

	while (h != NULL)
	{
		printf("%d\n", h->n);
		count++;
		h = h->next;
	}

	return (count);
}
