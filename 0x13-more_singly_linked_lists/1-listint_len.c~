#include "lists.h"

/**
 * print_listint_len - function that prints number of elements of a list.
 * @h: a singly linked list
 * Return: the number of nodes
 */

size_t print_listint_len(const listint_t *h)
{
	size_t count = 0;

	while (h != NULL)
	{
		count++;
		h = h->next;
	}

	return (count);
}
